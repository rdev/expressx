{
  "name": "expressx",
  "version": "0.0.0-development",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=8",
    "yarn": "YARN NO LONGER USED - use npm instead."
  },
  "scripts": {
    "prepare": "rimraf dist && npm run build",
    "build": "babel -d dist/ src/",
    "test": "rimraf dist && npm run lint",
    "lint": "vave lint",
    "semantic-release": "semantic-release"
  },
  "bin": {
    "expressx": "bin/expressx.js"
  },
  "keywords": [],
  "author": "Max Rovensky <hey@fivepointseven.com> (https://fivepointseven.com/)",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "commitizen": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.6.1",
    "eslint-config-vave": "^0.4.0",
    "eslint-formatter-pretty": "^1.3.0",
    "prettier": "^1.14.3",
    "semantic-release": "^15.9.17"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "vave",
    "rules": {
      "security/detect-non-literal-fs-filename": "off"
    }
  },
  "prettier": {
    "printWidth": 100,
    "useTabs": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "singleQuote": true
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": "commonjs",
          "versions": {
            "node": "8"
          },
          "exclude": [
            "transform-async-to-generator",
            "transform-regenerator"
          ]
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-do-expressions": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0-beta.42",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-flow": "^7.0.0-beta.42",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/runtime": "^7.1.2",
    "@csstools/postcss-sass": "^3.0.0",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.4",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "commander": "^2.18.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cross-spawn": "^6.0.5",
    "cssnano": "^4.1.4",
    "del": "^3.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-hbs": "^1.0.4",
    "fbjs": "^1.0.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "helmet": "^3.13.0",
    "i18n": "^0.8.3",
    "is-docker": "^1.1.0",
    "livereload": "^0.7.0",
    "log-symbols": "^2.2.0",
    "node-sass-middleware": "^0.11.0",
    "ora": "^3.0.0",
    "postcss": "^7.0.5",
    "postcss-scss": "^2.0.0",
    "rimraf": "^2.6.2",
    "serve-favicon": "^2.5.0",
    "source-map-support": "^0.5.9",
    "spdy": "^3.4.7",
    "webpack": "^4.20.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}